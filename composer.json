{
    "name": "pinebucket/php-client",
    "description": "A small PHP client for pinebucket.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Pinebucket\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pinebucket\\Client\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "monolog/monolog": "^2.3",
        "psr/log": "^1.1"
    }
}
